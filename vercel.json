{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "src/index.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "src/index.ts",
      "methods": ["POST"]
    },
    {
      "src": "/api/chat/polling",
      "dest": "src/index.ts",
      "methods": ["POST"]
    },
    {
      "src": "/api/chat/session",
      "dest": "src/index.ts",
      "methods": ["POST"]
    },
    {
      "src": "/api/chat/:sessionId/message",
      "dest": "src/index.ts",
      "methods": ["POST"]
    },
    {
      "src": "/api/chat/:sessionId",
      "dest": "src/index.ts",
      "methods": ["DELETE"]
    },
    {
      "src": "/health",
      "dest": "src/index.ts",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
} 